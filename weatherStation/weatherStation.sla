            <ITEXT FONTSIZE="10" CH="For example, when loading new data onto the database: "/>
            <para PARENT="Body text" LINESP="13"/>
            <ITEXT FONTSIZE="10" CH="mysql -hlocalhost -uroot -praspberry -- local-infile --verbose squall &lt;"/>
            <ITEXT CH=" "/>
            <ITEXT FONTSIZE="10" CH=" ~/squall/scripts/sql/load_weather_data.sql myysql"/>
            <para PARENT="Body text" LINESP="13"/>
            <ITEXT FONTSIZE="10" CH="This command calls the MySQL database manager."/>
            <para PARENT="Body text" LINESP="13"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="10" CH="-hlocalhost"/>
            <ITEXT FONTSIZE="10" CH=" confirms we are working on our local machine (security is a big issue if we want to work remotely)."/>
            <para PARENT="Body text" LINESP="13"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="10" CH="-uroot"/>
            <ITEXT FONTSIZE="10" CH=" this is the database root user, not the root for the whole Raspberry Pi! "/>
            <para PARENT="Body text" LINESP="13"/>
            <ITEXT FONTSIZE="10" CH="On a full production machine we would not run as root. "/>
            <para PARENT="Body text" LINESP="13"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="10" CH="-praspberry"/>
            <ITEXT FONTSIZE="10" CH=" the password for the database root "/>
            <para PARENT="Body text" LINESP="13"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="10" CH="--local-infile"/>
            <ITEXT FONTSIZE="10" CH=" warns MySQL to expect some data within a file (you specify this but it is NOT the name of the datafile itself."/>
            <para PARENT="Body text" LINESP="13"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="10" CH="--verbose"/>
            <ITEXT FONTSIZE="10" CH=" if something goes wrong give us some clues. "/>
