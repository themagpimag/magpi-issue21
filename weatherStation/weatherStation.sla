            <ITEXT FONTSIZE="11" CH="Appendix Five "/>
            <para PARENT="paragraph headings"/>
            <para PARENT="Body text" LINESP="14"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="11" CH="Raspberry Pi "/>
            <para PARENT="Body text" LINESP="14"/>
            <ITEXT FONTSIZE="10" CH="I prepared a clean installation of Raspbian on a Raspberry Pi with only 256K memory. "/>
            <para PARENT="Body text" LINESP="12"/>
            <ITEXT FONTSIZE="10" CH="The Raspbian image (2013-12-20-wheezy-Raspbian.zip.) was transferred to the SD card using the linux command "/>
            <para PARENT="Body text" LINESP="12"/>
            <ITEXT FONTSIZE="10" CH="sudo dd bs=1M if=2013-12-20-wheezy-Raspbian.img of=/dev/sdb. "/>
            <para PARENT="Body text" LINESP="12"/>
            <ITEXT FONTSIZE="10" CH="I ran sudo raspi-config to expand the file system space, set the locale, the hostname and the memory ballance to 16."/>
            <trail PARENT="Body text" LINESP="12"/>
